    </parameter>
    <parameter name="shovill-1-adv.mincov" defaultValue="0">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/iuc/shovill/shovill/1.0.4" parameterName="adv.mincov"/>
